{
  "name": "@teamwork/ot-rich-text",
  "version": "4.0.0",
  "description": "Format for representing rich-text documents and changes.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "test": "tap test/**/*.js",
    "tdd": "nodemon ./node_modules/.bin/tap test/*.js",
    "test-coverage": "npm test -- --cov --coverage-report=lcov",
    "tdd-coverage": "nodemon ./node_modules/.bin/tap --cov --coverage-report=lcov --no-browser test/*.js",
    "tap": "tap",
    "tap-watch": "nodemon ./node_modules/.bin/tap",
    "preversion": "npm test",
    "postversion": "git push && git push origin v${npm_package_version}"
  },
  "author": "Greg Kubisa <gkubisa@gmail.com>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git://github.com/Teamwork/ot-rich-text.git"
  },
  "files": [
    "index.js",
    "lib/"
  ],
  "devDependencies": {
    "nodemon": "^1.12.0",
    "ot-fuzzer": "^1.1.0",
    "tap": "^10.7.2"
  },
  "dependencies": {
    "fast-diff": "^1.1.2"
  }
}
